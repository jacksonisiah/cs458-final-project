{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "coreclr",
        "request": "launch",
        "name": "Debug",
        "program": "${workspaceFolder}/${input:p}/bin/Debug/net9.0/${input:p}.dll",
        "cwd": "${workspaceFolder}/${input:p}",
        "preLaunchTask": "build"
      }
    ],
    "inputs": [
      {
        "type": "pickString",
        "id": "p",
        "description": "Select the project to debug",
        "options": [
          "FinalProject.MVC"
        ]
      }
    ]
  }